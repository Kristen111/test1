<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">




    <servlet>
        <servlet-name>registServlet</servlet-name>
        <servlet-class>com.aistar.controller.customer.RegistServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>registServlet</servlet-name>
        <url-pattern>/customer/regist</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CheckTelnoServlet</servlet-name>
        <servlet-class>com.aistar.controller.CheckTelnoServlet</servlet-class>

    </servlet>

    <servlet-mapping>
        <servlet-name>CheckTelnoServlet</servlet-name>
        <url-pattern>/cust/checkTelno</url-pattern>
    </servlet-mapping>




</web-app>
